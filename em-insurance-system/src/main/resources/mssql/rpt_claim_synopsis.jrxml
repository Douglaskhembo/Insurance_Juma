	<queryString language="SQL">
		<![CDATA[select  org_name,org_mobile ,org_phone,org_address,org_website,phy_address,add_email_addr,
 s3.client_fname,s3.client_onames,s5.pol_client_pol_no,s2.risk_sht_desc,s2.risk_desc,
s2.risk_wef_date,s2.risk_wet_date,s.clm_no,s6.acct_name,s5.pol_sum_insur_amt,
sum (case when s7.trans_dc = 'D' 
then s7.trans_balance
end) as Ins_bal,
sum (case when s7.trans_dc = 'C' 
then s7.trans_balance
end)*-1 as client_bal,
s8.pr_desc, s9.cov_desc, s.clm_loss_date,s.clm_time,s.clm_booked_dt,s.clm_insurer_dt,
s.clm_next_rvw_dt,s.clm_status,s6.acct_contact_person,s6.acct_contact_title,
s6.acct_email,s6.acct_phone,s4.user_name,s.clm_loss_desc,
case when clm_clmnt_tp = 'S'
then s10.client_fname+' '+s10.client_onames
end as claimants,
case when clm_clmnt_tp = 'T'
then s11.clmnt_othernames+' '+s11.clmnt_surname
end as oClaimants, s12.clm_prl_total_reserve claim_estimate
FROM sys_brk_organization,sys_brk_clm_bookings s
INNER JOIN sys_brk_clm_claimants s1 on s.clm_id = s1.clm_clmnt_clm_id
inner join sys_brk_risks s2 on s2.risk_id = s.clm_risk_id
inner join sys_brk_clients s3 on s3.client_id = s2.risk_insured_id
inner join sys_brk_users s4 on  s4.user_id = s.clm_booked_by
inner join sys_brk_policies s5 on s5.pol_id = s2.risk_pol_id
inner join sys_brk_accounts s6 on s6.acct_id = s5.pol_agent_id
left outer join sys_brk_main_transactions s7 on s5.pol_id = s7.trans_pol_id
inner join sys_brk_products s8 on s8.pr_code = s5.pol_prod_id
inner join sys_brk_covertypes s9 on s9.cov_id = s2.risk_cover_id
LEFT OUTER  JOIN sys_brk_clients s10  on  s10.client_id = s1.clm_clmnt_client_id
LEFT OUTER join sys_brk_claimants s11 on s11.clmnt_id = s1.clm_clmnt_clmnt_id
inner join sys_brk_clm_perils s12 on s.clm_id = s12.clm_prl_clm_id
where s.clm_id = $P{clmid} 
group by clm_clmnt_tp,org_name,org_mobile ,org_phone,org_address,org_website,phy_address,add_email_addr,
s3.client_fname,s3.client_onames,s5.pol_client_pol_no,s2.risk_sht_desc,s2.risk_desc,
s2.risk_wef_date,s2.risk_wet_date,s.clm_no,s6.acct_name,s5.pol_sum_insur_amt,
s8.pr_desc, s9.cov_desc, s.clm_loss_date,s.clm_time,s.clm_booked_dt,s.clm_insurer_dt,
s.clm_next_rvw_dt,s.clm_status,s6.acct_contact_person,s6.acct_contact_title,
s6.acct_email,s6.acct_phone,s4.user_name,s.clm_loss_desc,
s10.client_fname+' '+s10.client_onames,s11.clmnt_othernames+' '+s11.clmnt_surname,
s12.clm_prl_total_reserve]]>
	</queryString>
